			<mvc:XMLView id="beginView" viewName="BuildingPermits.view.Detail"/>
		</beginColumnPages>
		<midColumnPages>
			<mvc:XMLView id="detailView" viewName="BuildingPermits.view.Detaildetail"/>
		</midColumnPages>
	</FlexibleColumnLayout>
</mvc:View>